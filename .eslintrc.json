{
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["*.js"],
      "extends": ["eslint:recommended"]
    }
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 0
  }
}
